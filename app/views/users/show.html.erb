<div class="page-header">
  <h1>User #<%= @user.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Username</dt>
  <dd><%= @user.username %></dd>
  <dd>
  <ul>
    <% @user_favorites.each do |favorite| %>
      <li><strong><%= Dish.find(favorite.dish_id).name %></strong> at <strong><%= Venue.find(favorite.venue_id).name %></strong></li>
    <% end %>
  </ul>
</dd>


</dl>
