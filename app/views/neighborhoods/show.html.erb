<div class="page-header">
  <h1>Neighborhood #<%= @neighborhood.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @neighborhood.name %></dd>

  <dt>City</dt>
  <dd><%= @neighborhood.city %></dd>

  <dt>Venues</dt>
  <dd>
    <ul>
      <% @neighborhood_venues.each do |venue| %>
        <li><%= venue.name %></li>
      <% end %>
    </ul>
  </dd>

</dl>
